// Lab #5 task 4

function [nsd, k]=nsdk(a,b)
    k = 0
    while (a ~=0) & (b ~= 0)
        if a>b then
            a = modulo(a,b)
        else
            b = modulo(b,a)
        end
        k=k+1
    end
    nsd = a+b
endfunction

[v,n] = nsdk(124,56)
disp('gcd(124,56) = '+string(v)+' , calculated in '+string(n)+' steps')

for k=1:20
    found = %F
    for i=1:10**5
        for j=1:10**5
            if i>j then
                [vv,nn] = nsdk(i,j)
                if nn == k then
                    disp(string(k)+'. '+string(i)+' '+string(j))
                    found = %T
                    break
                end
            end
        end
        if found then
            break
        end
    end
end

