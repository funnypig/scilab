function res=mishDob(v1,v2,v3)
    res = det([v1;v2;v3])
endfunction

function b=ifCompl(v1, v2, v3)
    if mishDob(v1,v2,v3) == 0 then
        b = %T
    else
        b = %F
    end
endfunction

function b=onePlain(x1, y1, z1, x2, y2, z2, x3, y3, z3, x4, y4, z4)
    v1 = [x2-x1 y2-y1 z2-z1]
    v2 = [x3-x1 y3-y1 z3-z1]
    v3 = [x4-x1 y4-y1 z4-z1]
    if ifCompl(v1, v2, v3) then
        b = %T
    else
        b = %F
    end
endfunction
