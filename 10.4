// 10.4
function res=planes(a1,b1,c1,d1,a2,b2,c2,d2)
    a = a1/a2
    b = b1/b2
    c = c1/c2
    if (a == b) & (b == c) & (a == c) then
        if d1 == d1 then
            res = 'equal'
        else
            res = '||'
        end
    else
        res = 'intersection'
    end
endfunction

